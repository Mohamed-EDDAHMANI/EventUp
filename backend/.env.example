# .env.example for NestJS Backend
# MongoDB connection string
MONGO_URI=mongodb://root:example@mongo:27017/eventup?authSource=admin

# JWT secret for authentication
JWT_SECRET=your_jwt_secret_here

# JWT expiration time (e.g., 1d, 12h)
JWT_EXPIRES_IN=1d

# Application port
PORT=3001

# Other environment variables as needed
# Example: EMAIL_SERVICE_API_KEY=your_email_service_key
